<h1 class="my-5">Home</h1>
<button type="button" class="btn btn-outline-secondary" id="btn">Count Images</button>
<script>
  const btn = document.querySelector('#btn');
  btn.addEventListener('click', () => {
    fetch('/home/images/')
    .then(response => response.json())
    .then((result) => {
      btn.textContent = result + ' image(s)';
    });
  });
</script>
</div>
